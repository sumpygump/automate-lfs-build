#!/bin/bash

source "$(dirname "$0")"/../common/common_source

NAME=gperf
VERSION=$GPERF_VER
TARFILE=$NAME-$VERSION.tar.gz
FETCH_URL="http://ftp.gnu.org/gnu/gperf/$TARFILE"
mSBU=50

configure()
{
    cd "$WORKBENCH/$NAME-$VERSION"
    mkdir build; cd build; BUILD_PATH=$(pwd)

    ../configure --prefix=/usr --docdir=/usr/share/doc/gperf-3.1 >>"$LOGFILE" 2>&1
}

source "$(dirname "$0")"/../common/common_exec
