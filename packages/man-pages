#!/bin/bash

source "$(dirname "$0")"/../common/common_source

NAME=man-pages
VERSION=$MANPAGES_VER
TARFILE=$NAME-$VERSION.tar.xz
FETCH_URL="https://www.kernel.org/pub/linux/docs/man-pages/$TARFILE"
mSBU=10

configure()
{
    # Set only the build-path
    BUILD_PATH="$WORKBENCH/$NAME-$VERSION" 
}

build()
{
    # Do Nothing
    :
}

build_install()
{
    cd $BUILD_PATH
    make prefix=/usr install >>"$LOGFILE" 2>&1
}

source "$(dirname "$0")"/../common/common_exec
