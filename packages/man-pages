#!/bin/bash

source "$(dirname "$0")"/../common/common_source

NAME=man-pages
VERSION=${MANPAGES_VER}
TARFILE=${NAME}-${VERSION}.tar.xz
# Older versions (<5.x) are in the /Archive/ path
FETCH_URL="https://mirrors.edge.kernel.org/pub/linux/docs/man-pages/Archive/$TARFILE"
mSBU=30

configure()
{
    # Set only the build-path
    BUILD_PATH="${TMP}"/"${NAME}"-"${VERSION}" 
}

build()
{
    # Do Nothing
    :
}

source "$(dirname "$0")"/../common/common_exec
