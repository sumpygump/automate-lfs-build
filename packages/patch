#!/bin/bash

source "$(dirname "$0")"/../common/common_source

NAME=patch
VERSION=$PATCH_VER
TARFILE=$NAME-$VERSION.tar.xz
FETCH_URL="http://ftp.gnu.org/gnu/patch/$TARFILE"
SBUx10=2

configure()
{
    default_configure_chroot "$TMP"/"$NAME"-"$VERSION"
}

source "$(dirname "$0")"/../common/common_exec
