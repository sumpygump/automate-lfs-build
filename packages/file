#!/bin/bash

source "$(dirname "$0")"/../common/common_source

NAME=file
VERSION=$FILE_VER
TARFILE=$NAME-$VERSION.tar.gz
FETCH_URL="ftp://ftp.astron.com/pub/file/$TARFILE"
mSBU=100

configure()
{
    default_configure_chroot "$WORKBENCH/$NAME-$VERSION"
}

source "$(dirname "$0")"/../common/common_exec
