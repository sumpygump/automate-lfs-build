#!/bin/bash

source "$(dirname "$0")"/../common/common_source

NAME=XML-Parser
VERSION=$XMLPARSER_VER
TARFILE=$NAME-$VERSION.tar.gz
FETCH_URL="https://cpan.metacpan.org/authors/id/T/TO/TODDR/$TARFILE"
mSBU=100

configure()
{
    cd "$WORKBENCH/$NAME-$VERSION"
    BUILD_PATH=$(pwd)
    perl Makefile.PL >>"$LOGFILE" 2>&1
}

source "$(dirname "$0")"/../common/common_exec
