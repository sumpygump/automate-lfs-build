#!/bin/bash

source "$(dirname "$0")"/../common/common_source

NAME=lfs-bootscripts
VERSION=20210608
TARFILE=$NAME-$VERSION.tar.xz
FETCH_URL="https://www.linuxfromscratch.org/lfs/downloads/11.0/$TARFILE"
mSBU=50

configure()
{
    cd "$WORKBENCH/$NAME-$VERSION"
    BUILD_PATH=$(pwd)
    return 0
}

build()
{
    # do nothing for build
    return 0
}

source "$(dirname "$0")"/../common/common_exec
