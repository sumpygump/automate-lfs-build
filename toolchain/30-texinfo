#!/bin/bash

source "$(dirname "$0")"/../common/common_source

NAME=texinfo
VERSION=${TEXINFO_VER}
TARFILE=$NAME-$VERSION.tar.xz
FETCH_URL="http://ftp.gnu.org/gnu/texinfo/$TARFILE"
mSBU=300

configure()
{
    default_configure "$TMP"/"$NAME"-"$VERSION"
}

source "$(dirname "$0")"/../common/common_exec
